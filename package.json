{
  "name": "ns-request",
  "version": "1.0.0",
  "description": "ns-request",
  "main": "dist/main.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "build": "cross-env BABEL_ENV=production webpack --progress --env=production",
